> --- a/arch/arm/boot/dts/bcm2835-rpi.dtsi
> +++ b/arch/arm/boot/dts/bcm2835-rpi.dtsi
> @@ -14,6 +14,9 @@ act {
>  	soc {
>  		firmware: firmware {
>  			compatible = "raspberrypi,bcm2835-firmware", "simple-bus";
> +			#address-cells = <1>;
> +			#size-cells = <1>;
> +
>  			mboxes = <&mailbox>;
>  			dma-ranges;
>  		};
